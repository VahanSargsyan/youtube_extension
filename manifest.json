{
  "manifest_version": 2,
  "name": "Youtube control panel",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*watch*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Youtube control panel"
  }
}
